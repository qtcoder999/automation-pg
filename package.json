{
  "name": "@drazik/eslint-prettier-starter",
  "version": "1.0.0",
  "description": "A starter kit that includes linting and reformatting JS files as a precommit hook",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "precommit": "lint-staged",
    "start": "node app.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/drazik/eslint-prettier-starter.git"
  },
  "keywords": [
    "starter",
    "eslint",
    "prettier"
  ],
  "author": "Cyrille Perois <cperois@outlook.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/drazik/eslint-prettier-starter/issues"
  },
  "homepage": "https://github.com/drazik/eslint-prettier-starter#readme",
  "devDependencies": {
    "@wandiparis/eslint-config-wandi": "^2.0.0",
    "eslint": "^4.19.1",
    "eslint-config-prettier": "^2.10.0",
    "eslint-plugin-import": "^2.17.2",
    "husky": "^0.14.3",
    "lint-staged": "^4.3.0",
    "prettier": "^1.17.1"
  },
  "lint-staged": {
    "*.js": [
      "eslint --fix",
      "prettier --write",
      "git add"
    ]
  },
  "dependencies": {
    "axios": "^0.18.0",
    "excel4node": "^1.7.2"
  }
}
